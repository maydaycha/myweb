              <div class="row">
                <div class="col-md-3 text-right-md text-left-sm">
                  <strong><span class="Corange">* </span>案件名稱</strong>
                </div>
                <div class="col-md-9">
                  <div class="form-group lable-group">
                    <!-- <input name="name" type="text" class="form-control" required> -->
                    <%= f.text_field :name, :required => true, :class => "form-control", :id => "project_name" %>
                    <label for="name">為您的案件命名</label>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-3 text-right-md text-left-sm">
                  <strong><span class="Corange">* </span> 工作內容</strong>
                </div>
                <div class="col-md-9">
                  <div class="form-group lable-group">
                    <!-- <textarea  name="detail" class="form-control" rows="5" required></textarea> -->
                    <%= f.text_area :description, :required => true, :class => "form-control", :id => "project_description" %>
                    <label for="detail">描述您的案件工作內容</label>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-3 text-right-md text-left-sm">
                  <strong>案件技能</strong>
                </div>
                <div class="col-md-9">
                  <!-- <div class="form-group lable-group"> -->
                  <!-- <input name="skill" type="text" class="form-control" required> -->
                  <!-- <%= f.text_field :require_skills, :required => true, :class => "form-control" %>-->
                  <input id="skill-tags" type="text" name="project[require_skills]" class="form-control tags" value="<%= current_user.user_skills.map{|e| e.name }.join(", ") %>" defaultText="請輸入案件需求技能" alert-limit="<%= I18n.t("page.profile_show.tabA_segment3_alert_limit")%>" />
                  <!-- </div> -->
                </div>
              </div>
              <div class="row">
                <div class="col-md-3 text-right-md text-left-sm "><strong>急件</strong></div>
                <div class="col-md-9 text-left">
                  <div class="form-group">
                    <div class="marpad-left text-left">
                      <%= f.check_box :is_emergency %> 我需要急件處理
                    </div>
                  </div>
                </div>
              </div>
              <hr>
              <div><!-- BEGIN CASE TYPE -->
                <div class="row bottom30">
                  <div class="col-md-8 col-md-offset-1">
                    <div class="row">

                      <div class="col-md-3">
