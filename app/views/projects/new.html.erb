      <div class="tab-pane fade in " id="two"> <!--BEGIN PANEL 02-->
        <div class="form-style">
          <%= form_for @project, url: projects_path(@project), method: :post, html: {class: "go-bottom", id: "submit_form"} do |f| %>

          <div class="portlet box yellow"> <!--BEGIN PORTLET YELLOW 01-->
            <div class="portlet-title">
              <div class="title-tex"> 選擇案件類別 </div>
            </div>
            <div class="portlet-body">
              <div class="row">
                <div class="col-sm-6">
                  <div class="form-group">
                    <p>主類別</p>
                    <div class="top5">
                      <select name="project[main_skill]" id="mainClass_select" class="form-control">
                        <option value="-1">所有類別</option>
                        <% t(:main_skill_category).each_with_index do |main_one, index| %>
                        <% if index == @main_skill_id %>
                        <option value="<%= index %>" selected><%= main_one['name'] %></option>
                        <% else %>
                        <option value="<%= index %>"><%= main_one['name'] %></option>
                        <% end %>
                        <% end %>
                      </select>
                    </div>
                  </div>
                </div>
                <div class="col-sm-6">
                  <div class="form-group">
                    <p>子類別</p>
                    <div class="top5">
                      <select name="project[sub_skill]" id="subClass_select2" class="form-control" >
                        <option value="-1">所有類別</option>
                        <% t(:sub_skill_category)[@main_skill_id].each_with_index do |sub_one, index| %>
                        <% if index == @sub_skill_id %>
                        <option value="<%= index %>" selected><%= sub_one['name'] %></option>
                        <% else %>
                        <option value="<%= index %>"><%= sub_one['name'] %></option>
                        <% end %>
                        <% end %>
                      </select>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div><!--End PORTLET YELLOW 01-->
          <div class="portlet box yellow"> <!--BEGIN PORTLET YELLOW 02-->
